@model IEnumerable<_744Project.Models.Store>

@{
    ViewBag.Title = "Index";
}

<div class="container">
    <h2>Index</h2>
    <div class="panel panel-default">
        <div class="panel-body">

            <p>
                @Html.ActionLink("Create New", "Create")
            </p>
            <table class="table">
                <tr>
                    <th>
                        @Html.DisplayNameFor(model => model.Region.regionName)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Relay.relayName)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.storeIP)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.storeName)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.storeWeight)
                    </th>
                    <th></th>
                </tr>

                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.Region.regionName)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Relay.relayName)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.storeIP)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.storeName)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.storeWeight)
                        </td>
                        <td>
                            @*@Html.ActionLink("Edit", "Edit", new { id = item.storeID }) |
                            @Html.ActionLink("Details", "Details", new { id = item.storeID }) |
                            @Html.ActionLink("Delete", "Delete", new { id = item.storeID })*@
                        </td>
                    </tr>
                }

            </table>

</div>
</div>
</div>
